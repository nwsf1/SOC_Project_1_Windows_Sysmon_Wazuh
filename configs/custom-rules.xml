<group name="windows, sysmon, custom_rules">

  <rule id="900001" level="7">
    <if_sid>61603</if_sid>
    <description>Suspicious PowerShell execution detected</description>
    <match>powershell.exe</match>
    <options>no_full_log</options>
    <mitre>
        <id>T1059.001</id>
    </mitre>
  </rule>

  <rule id="900002" level="10">
    <if_sid>61601</if_sid>
    <description>Unsigned EXE created in user directory</description>
    <regex>Users\\.*\\.exe</regex>
    <mitre>
        <id>T1204</id>
    </mitre>
  </rule>

</group>
